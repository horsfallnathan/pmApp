<div class="page-head flexed-row flex-space-between">
  <div class="page-title text-left width-60">
    <h3>Your Current Projects</h3>
  </div>
  <div class="search-bar width-20">
    <input type="text" name="searchProject" id="searchProject" placeholder="Start typing...">
    <label for="searchProject">Search Project</label>
  </div>
</div>

{{#each data}}
<div class="project-List">
  <div class="card">
    <div class="card-content">
      <span class="card-title activator">{{projectName}}
        <i class="material-icons color-blue right">add_box</i></span>
      <p><a href="/project/{{id}}">View Project</a></p>
    </div>
    <div class="card-reveal">
      {{!-- {{#each data}} --}}
      <span class="card-title grey-text text-darken-4">{{projectName}}<i class="material-icons right">close</i></span>
      {{#each tasks}}
      <p class="tb-margin tb-border">{{title}}</p>
      {{/each}}
      {{!-- {{/each}} --}}
    </div>
  </div>
</div>
{{/each}}

<script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>